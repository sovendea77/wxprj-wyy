.playlist-container{
  display:  flex;
  flex-wrap: wrap;
  justify-content:  space-around;
  flex-direction: row;
  margin-top: 15px;
}
.swiperImg{
  width: 100%;
  height: 100%;
}
.search-container {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  padding: 0 20rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  z-index: 99;
  box-sizing: border-box;
  -webkit-overflow-scrolling: touch;
}

.search-header {
  display: flex;
  align-items: center;
  width: 100%;
  height: 80 rpx;
}

.search {
  flex: 1;
}

.card-theme {
  --td-swiper-radius: 0;
  --td-swiper-item-padding: 0 12rpx;
  --td-swiper-nav-dot-color: var(--td-bg-color-component, #e7e7e7);
  --td-swiper-nav-dot-active-color: var(--td-brand-color, #0052d9);

  padding-bottom: 18px;
}

.card-theme .card-theme-nav {
  bottom: -18px;
}

.scale-candidate {
  height: 126px !important;
}

